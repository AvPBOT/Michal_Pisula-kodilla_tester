SELECT A.ID, A.SUMMARY, A.DESCRIPTION, B.NAME
FROM ISSUES A
JOIN ISSUESLISTS B ON A.ISSUESLIST_ID = B.ID;

SELECT A.SUMMARY, A.DESCRIPTION, U.ID, U.FIRSTNAME, U.LASTNAME
FROM USERS U
JOIN ISSUES A ON U.ID = A.USER_ID_ASSIGNEDTO;

SELECT U.FIRSTNAME, U.LASTNAME, COUNT(*) AS ISSUES_NUMBER
FROM USERS U
JOIN ISSUES A ON U.ID = A.USER_ID_ASSIGNEDTO
GROUP BY A.USER_ID_ASSIGNEDTO
HAVING COUNT(*) > 1
ORDER BY U.LASTNAME, U.FIRSTNAME;
